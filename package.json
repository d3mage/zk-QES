{
  "name": "private_voting_codespace",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/critesjosh/private_voting_codespace.git",
  "author": "Josh Crites <jc@joshcrites.com>",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "fees": "node --loader ts-node/esm scripts/fees.ts",
    "fees::testnet": "AZTEC_ENV=testnet node --loader ts-node/esm scripts/fees.ts",
    "clean": "rm -rf ./contracts/src/artifacts ./contracts/target",
    "clear-store": "rm -rf ./store",
    "codegen": "aztec codegen contracts/target --outdir contracts/src/artifacts",
    "compile": "cd contracts && ${AZTEC_NARGO:-aztec-nargo} compile && aztec-postprocess-contract",
    "compile:circuit:poseidon": "cd circuits/pades_ecdsa_poseidon && nargo compile",
    "compile:circuit:hybrid": "cd circuits/pades_ecdsa_hybrid && nargo compile",
    "hash-byte-range": "node --loader ts-node/esm scripts/hash-byte-range.ts",
    "extract-cms": "node --loader ts-node/esm scripts/extract-cms.ts",
    "extract-cades": "npx tsx scripts/extract-cades.ts",
    "prove": "node --loader ts-node/esm scripts/prove.ts",
    "prove:native": "node --loader ts-node/esm scripts/prove-native.ts",
    "verify": "node --loader ts-node/esm scripts/verify.ts",
    "merkle:build": "node --loader ts-node/esm tools/merkle/build.ts",
    "merkle:prove": "node --loader ts-node/esm tools/merkle/prove.ts",
    "merkle-poseidon:build": "node --loader ts-node/esm tools/merkle-poseidon/build.ts",
    "merkle-poseidon:prove": "node --loader ts-node/esm tools/merkle-poseidon/prove.ts",
    "eutl:fetch": "node --loader ts-node/esm tools/eutl/fetch.ts",
    "eutl:root": "node --loader ts-node/esm tools/eutl/root.ts",
    "pades:certify": "node --loader ts-node/esm scripts/pades-certify.ts",
    "pades:timestamp": "node --loader ts-node/esm scripts/pades-timestamp.ts",
    "pades:lt": "node --loader ts-node/esm scripts/pades-lt.ts",
    "e2e-test": "node --loader ts-node/esm scripts/e2e-test.ts",
    "e2e-test-poseidon": "node --loader ts-node/esm scripts/e2e-test-poseidon.ts",
    "benchmark": "npx tsx scripts/benchmark.ts",
    "test": "yarn test:js && yarn test:nr",
    "test::testnet": "AZTEC_ENV=testnet yarn test:js",
    "test:js": "rm -rf store/pxe && NODE_NO_WARNINGS=1 node --experimental-vm-modules $(yarn bin jest) --no-cache --runInBand --config jest.integration.config.json",
    "test:nr": "cd contracts && VERSION=3.0.0-devnet.5 aztec test",
    "update": "aztec update --contract contracts && ./.github/scripts/update_contract.sh $(grep -oP 'tag\\s*=\\s*\"\\K[^\"]+' \"contracts/Nargo.toml\" | head -1) && yarn && yarn update-readme-version",
    "update-readme-version": "node ./.github/scripts/update-readme-version.js"
  },
  "dependencies": {
    "@aztec/accounts": "3.0.0-devnet.5",
    "@aztec/aztec.js": "3.0.0-devnet.5",
    "@aztec/bb.js": "3.0.0-nightly.20251114",
    "@aztec/foundation": "^3.0.0-devnet.5",
    "@aztec/noir-contracts.js": "3.0.0-devnet.5",
    "@aztec/protocol-contracts": "3.0.0-devnet.5",
    "@aztec/pxe": "3.0.0-devnet.5",
    "@aztec/stdlib": "3.0.0-devnet.5",
    "@noir-lang/noir_js": "1.0.0-beta.15",
    "@peculiar/asn1-cms": "^2.3.8",
    "@peculiar/asn1-schema": "^2.3.8",
    "@peculiar/asn1-x509": "^2.3.8",
    "asn1js": "^3.0.6",
    "axios": "^1.12.2",
    "circomlibjs": "^0.1.7",
    "dotenv": "^17.2.2",
    "ethers": "^6.15.0",
    "ipfs-http-client": "^60.0.1",
    "pdf-lib": "^1.17.1",
    "pedersen-fast": "^0.4.0",
    "pkijs": "^3.3.1",
    "poseidon-lite": "^0.3.0",
    "pvutils": "^1.1.3",
    "winston": "^3.18.3",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/mocha": "^10.0.6",
    "@types/node": "^22.15.1",
    "@types/xml2js": "^0.4.14",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.4"
  },
  "jest": {
    "testTimeout": 200000
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
